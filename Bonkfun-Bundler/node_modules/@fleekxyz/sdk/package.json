{
  "name": "@fleekxyz/sdk",
  "description": "Fleek Platform NodeJS SDK",
  "version": "1.4.2",
  "main": "./dist/index.node.js",
  "browser": "./dist/index.browser.js",
  "types": "./dist-types/index.d.ts",
  "publishConfig": {
    "directory": "output"
  },
  "devDependencies": {
    "@fleekxyz/sites": "^1.2.1",
    "@fleekxyz/utils": "^0.1.4",
    "@types/detect-node": "^2.0.0",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/lodash": "^4.14.191",
    "@types/luxon": "^3.1.0",
    "@types/whatwg-fetch": "^0.0.33",
    "@vitest/coverage-v8": "1.3.1",
    "@web-std/file": "^3.0.2",
    "concurrently": "^7.6.0",
    "decimal.js": "^10.4.3",
    "esbuild": "^0.16.9",
    "isomorphic-formdata": "link:@types/@willdurand/isomorphic-formdata",
    "ts-node": "^10.9.1",
    "typescript": "4.9.3",
    "vitest": "1.3.1",
    "@fleekxyz/auth": "1.1.2",
    "@fleekxyz/gql-client-utils": "1.2.6",
    "@fleekxyz/env-guards": "1.2.1",
    "@fleekxyz/errors": "1.4.0",
    "@fleekxyz/tester": "1.3.1",
    "@fleekxyz/graphql": "1.8.2",
    "eslint-config-fleek-custom": "1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "doppler run --command='pnpm build:types && ts-node --files ./build.ts'",
    "build:ci": "pnpm run build",
    "build:types": "tsc -p tsconfig.types.json",
    "watch": "tsc --watch",
    "lint": "eslint --fix ./build.ts --ext .js,.jsx,.ts,.tsx src",
    "tsc": "tsc --noEmit",
    "test:disabled": "vitest run --dir src",
    "increaseVersion": "pnpm version prerelease --preid dev",
    "release": "pnpm publish --access=public --no-git-checks",
    "clean": "rm -rf node_modules dist* output generated"
  }
}